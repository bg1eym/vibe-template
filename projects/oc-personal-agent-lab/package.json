{
  "name": "oc-personal-agent-lab",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "run:parse": "tsx pipelines/parse_conversation.ts",
    "run:match": "tsx pipelines/match_stub.ts",
    "eval": "tsx eval/run_eval.ts",
    "oc:smoke": "tsx runtime/oc_bridge.ts",
